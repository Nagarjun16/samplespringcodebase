            <xml-property name="queryText"><![CDATA[select PaymentMode, 
	   isnull(mst_bank.BankCode, IssuingBank) as IssuingBank,
	   isnull(TransactionReferenceNumber, '') as TransactionReferenceNumber,
	   sum(Billing_Payment.PaymentAmount) as Amount, 
	   Billing_PaymentReceipt.CounterNumber,
	   Billing_Payment.verifiedon,
	   Billing_Payment.TransactionReferenceDate 'Cheque Date',
       convert(datetime, CONVERT(datetimeoffset, Billing_Payment.verifiedon) AT TIME ZONE 
		(select ParameterValueChar from App_SystemParameters where ParameterCode='Station_timezone')) 'verified time',		
	   Billing_Payment.LastUpdatedUserCode 'verified by',
	   Billing_PaymentReportNumber.VerifiedBlockNo reportNo,
	   Billing_PaymentReportNumber.ReportGeneratedOn,
	   REPLACE(REPLACE((select top 1 ParameterLongDescription from App_SystemParameters where ParameterCode='FOR_CREDIT_OF'), CHAR(13), ' '), CHAR(10), ' ') FOR_CREDIT_OF,
(select top 1 ParameterLongDescription from App_SystemParameters where ParameterCode='BANK_ACCOUNT_NUM') BANK_ACCOUNT_NUM,
(select top 1 ParameterLongDescription from App_SystemParameters where ParameterCode='BANK_NAME') BANK_NAME	  
from Billing_Payment
inner join Billing_PaymentReportNumber on Billing_PaymentReportNumber.VerifiedBlockNo = Billing_Payment.VerifiedBlockNo
inner join Billing_PaymentReceipt on (Billing_Payment.PaymentReceiptId  =  Billing_PaymentReceipt.PaymentReceiptId AND Billing_PaymentReportNumber.reportType = 'Banking Slip')
left join mst_bank on mst_bank.BankName=Billing_Payment.IssuingBank
where
	CollectionStatus  = 'Verified'
	and Billing_PaymentReportNumber.ReportControlNo=convert(int,?)
AND PaymentMode ='CHEQUE'

group by Billing_PaymentReceipt.CounterNumber, Billing_Payment.TransactionReferenceDate, Billing_Payment.LastUpdatedUserCode,
Billing_Payment.verifiedon, PaymentMode, isnull(mst_bank.BankCode, IssuingBank) , isnull(TransactionReferenceNumber, ''),
Billing_PaymentReceipt.ReceiptNumber,Billing_Payment.verifiedon,Billing_PaymentReportNumber.VerifiedBlockNo,Billing_PaymentReportNumber.ReportGeneratedOn

order by Billing_PaymentReceipt.ReceiptNumber, TransactionReferenceNumber
]]></xml-property>

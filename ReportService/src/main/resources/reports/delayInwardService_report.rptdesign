            <xml-property name="queryText"><![CDATA[with inward as(select distinct
flt_operativeflight.flightkey,
format(cast (flt_operativeflight_legs.dateSTA as date),'ddMMMyyyy') as flightdate,
flt_operativeflight.flight_id,
case when InwardServiceReportFinalizedBy is not null then 'Y' else 'N' END as FinalizeFlag,
case when FlightDiscrepncyListSentBy is not null then 'Y' else 'N' END as FinalizeFlag_FDL,
 ship.DiscrepancyType,
 Substring(ship.ShipmentNumber, 0, 4) AS inwardawbpfx,
  Substring(ship.ShipmentNumber, 4, 11) AS inwardAwbnum,
  ship.Origin
,ship.Destination
,ship.NatureOfGoodsDescription as content
,ship.IrregularityDescription as inward_natureofdiscrepancy
,ship.Pieces
,ship.Weight
,Imp_FlightEvents.RampCheckInCompletedAt
,Imp_FlightEvents.BreakDownCompletedAt
,Imp_FlightEvents.DocumentVerificationCompletedAt
,Imp_FlightEvents.BreakDownCompletedBy
,Imp_FlightEvents.DocumentVerificationCompletedBy
,Imp_FlightEvents.RampCheckInCompletedBy
,other.Remarks
,other.CreatedUserCode
from
flt_operativeflight inner join
 flt_operativeflight_legs  on flt_operativeflight_legs.Flight_Id = flt_operativeflight.Flight_Id
inner join imp_inwardservicereport on
imp_inwardservicereport.FlightId = flt_operativeflight.Flight_Id
  inner join
Imp_FlightEvents on Flt_OperativeFlight_Legs.Flight_ID=
Imp_FlightEvents.FlightId
 INNER JOIN
    mst_associatecarrierwithgroup
    ON mst_associatecarrierwithgroup.carriercode = flt_operativeflight.carriercode
  INNER JOIN
    mst_carriergroup
    ON mst_associatecarrierwithgroup.mst_carriergroup_id = mst_carriergroup.mst_carriergroup_id
left join  imp_inwardservicereportshipmentdiscrepancy ship
on imp_inwardservicereport.ImpInwardServiceReportId = ship.ImpInwardServiceReportId 
left join  Imp_InwardServiceReportOtherDiscrepancyInfo other
on imp_inwardservicereport.ImpInwardServiceReportId = other.ImpInwardServiceReportId
where  ship.ShipmentType = 'AWB' 
AND mst_carriergroup.carriergroupcode = ?
and Flt_OperativeFlight_Legs.DateSTA < ?
and ((Imp_FlightEvents.DocumentVerificationCompletedAt between ? and ?) or 
(Imp_FlightEvents.BreakDownCompletedAt between ? and ?) or
(Imp_FlightEvents.RampCheckInCompletedAt between ? and ?)
)
and Imp_FlightEvents.DocumentVerificationCompletedAt is not null  and
				Imp_FlightEvents.BreakDownCompletedAt is not null 


and Flt_OperativeFlight_Legs.FlightOffPoint='SIN'
)
select 
inward.flightkey,
 inward.flightdate,
inward.flight_id,
inward.FinalizeFlag,
inward.FinalizeFlag_FDL,
 inward.DiscrepancyType,
 inward.inwardawbpfx,
 inward.inwardAwbnum,
 inward.Origin
,inward.Destination
,inward.content
,inward.inward_natureofdiscrepancy
,inward.Pieces
,inward.Weight
,inward.RampCheckInCompletedAt
,inward.BreakDownCompletedAt
,inward.DocumentVerificationCompletedAt
,inward.BreakDownCompletedBy
,inward.DocumentVerificationCompletedBy
,inward.RampCheckInCompletedBy
,inward.Remarks
,inward.CreatedUserCode
, LAG(inward.flightkey) OVER (ORDER BY inward.flight_id) tempFlight	
, LAG(inward.flightdate) OVER (ORDER BY inward.flight_id) tempFlightDate	
,ROW_NUMBER() OVER (PARTITION BY inward.flight_id ORDER BY inward.flight_id ) as seqNum from inward
order by inward.flight_id ]]></xml-property>
        </oda-data-set>
    </data-sets>

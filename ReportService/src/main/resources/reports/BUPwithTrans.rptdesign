            <xml-property name="queryText"><![CDATA[SELECT  
	format(ShipmentVerificationInfo.DateATA) ATA,
	ShipmentVerificationInfo.CarrierCode Carrier, 
	ShipmentVerificationInfo.FlightNumber FlightNumber,
	Shipment_Master.ShipmentNumber,
	sum(isnull(Delivery.Weight,0.0)+ isnull(ShipmentInventory.Weight,0.0)) TotalWeight,
    LocalIntact.weight as intact,
	LocalNonIntact.weight as nonIntact,
	TranshipmentIntact.weight as TrashipmentIntact,
	TranshipmentNonIntact.weight as TrashipmentnonIntact
	
from Shipment_Master INNER JOIN 
 (
   SELECT 
           distinct
			  Imp_ShipmentVerification.ShipmentId,
              Imp_ShipmentVerification.FlightId,
			  Flt_OperativeFlight.CarrierCode,
              Flt_OperativeFlight.FlightNumber,
              Flt_OperativeFlight_legs.DateSTA,
              Flt_OperativeFlight_legs.DateATA,
              Flt_OperativeFlight_legs.AircraftType,
              Imp_ShipmentVerification.BreakDownPieces,
              Imp_ShipmentVerification.BreakDownWeight
   from
     Imp_ShipmentVerification 
               INNER JOIN Flt_OperativeFlight
                             ON Imp_ShipmentVerification.FlightId = Flt_OperativeFlight.Flight_Id 
               INNER JOIN flt_operativeflight_legs 
                             ON flt_operativeflight_legs.flight_id=Flt_OperativeFlight.flight_id and  flt_operativeflight_legs.flightOffPoint=?
              Where Imp_ShipmentVerification.breakdownpieces is not null
)ShipmentVerificationInfo ON ShipmentVerificationInfo.ShipmentId=Shipment_Master.ShipmentId LEFT JOIN
(SELECT distinct
	  sum(Shipment_Inventory.pieces) Pieces,
	  sum(Shipment_Inventory.weight) Weight,
	  Shipment_Inventory.Shipment_id,
	  Shipment_Inventory.flight_Id
FROM Shipment_Inventory INNER JOIN Shipment_Master on Shipment_Inventory.Shipment_id=Shipment_Master.ShipmentId
where Shipment_Master.Destination = ? and
       Shipment_Master.ShipmentType in ('AWB') and
       Shipment_Master.CancelledOn is null
GROUP BY Shipment_Inventory.Shipment_id,
  Shipment_Inventory.flight_Id)
ShipmentInventory ON ShipmentInventory.Shipment_id=ShipmentVerificationInfo.ShipmentId and ShipmentInventory.flight_id=ShipmentVerificationInfo.flightId Left join

(
  SELECT  distinct 
       Shipment_Master.ShipmentiD,
       Imp_ArrivalManifestShipmentInfo.weight       
    FROM
             
			   Imp_ArrivalManifestULD 
			    INNER JOIN 
			   Imp_ArrivalManifestShipmentInfo on Imp_ArrivalManifestShipmentInfo.ImpArrivalManifestULDId =Imp_ArrivalManifestULD.ImpArrivalManifestULDId
			    INNER JOIN 
			   Shipment_Master on Imp_ArrivalManifestShipmentInfo.ShipmentNumber=Shipment_master.ShipmentNumber and Imp_ArrivalManifestShipmentInfo.ShipmentDate=Shipment_master.ShipmentDate
			    INNER JOIN
               Shipment_MasterSHC ON Shipment_Master.ShipmentId = Shipment_MasterSHC.ShipmentId 
				INNER JOIN
               Mst_AssociateSHCByHandlingGroup ON Shipment_MasterSHC.SpecialHandlingCode = Mst_AssociateSHCByHandlingGroup.SpecialHandlingCode 
			    INNER JOIN
               Mst_SHCHandlingGroup ON Mst_AssociateSHCByHandlingGroup.MstSHCHandlingGroupID = Mst_SHCHandlingGroup.MstSHCHandlingGroupID
			    INNER JOIN
			   Imp_InboundContainers ON  Imp_ArrivalManifestULD.ULDNumber=Imp_InboundContainers.ULDNumber and Imp_InboundContainers.HandlingMode='BREAK'
    WHERE
			 Shipment_Master.Destination = ? and Imp_ArrivalManifestULD.ULDNumber is not null and Mst_SHCHandlingGroup.SHCHandlingGroupCode IN ('BUP','BUC')
			
  ) LocalIntact ON LocalIntact.ShipmentId=ShipmentVerificationInfo.ShipmentId LEFT JOIN
(
    SELECT  distinct 
       Shipment_Master.ShipmentiD,
       Imp_ArrivalManifestShipmentInfo.weight       
    FROM
             
			   Imp_ArrivalManifestULD 
			   INNER JOIN 
			   Imp_ArrivalManifestShipmentInfo on Imp_ArrivalManifestShipmentInfo.ImpArrivalManifestULDId =Imp_ArrivalManifestULD.ImpArrivalManifestULDId
			   INNER JOIN 
			   Shipment_Master on Imp_ArrivalManifestShipmentInfo.ShipmentNumber=Shipment_master.ShipmentNumber and Imp_ArrivalManifestShipmentInfo.ShipmentDate=Shipment_master.ShipmentDate
			    INNER JOIN
               Shipment_MasterSHC ON Shipment_Master.ShipmentId = Shipment_MasterSHC.ShipmentId 
				INNER JOIN
               Mst_AssociateSHCByHandlingGroup ON Shipment_MasterSHC.SpecialHandlingCode = Mst_AssociateSHCByHandlingGroup.SpecialHandlingCode 
			    INNER JOIN
               Mst_SHCHandlingGroup ON Mst_AssociateSHCByHandlingGroup.MstSHCHandlingGroupID = Mst_SHCHandlingGroup.MstSHCHandlingGroupID
			   INNER JOIN
			   Imp_InboundContainers ON  Imp_ArrivalManifestULD.ULDNumber=Imp_InboundContainers.ULDNumber and Imp_InboundContainers.HandlingMode='NOBREAK'
    WHERE
			 Shipment_Master.Destination = ? and Imp_ArrivalManifestULD.ULDNumber is not null and Mst_SHCHandlingGroup.SHCHandlingGroupCode IN ('BUP','BUC')
  ) LocalNonIntact ON LocalNonIntact.ShipmentId=ShipmentVerificationInfo.ShipmentId LEFT JOIN
 (
  SELECT  distinct 
       Shipment_Master.ShipmentiD,
       Imp_ArrivalManifestShipmentInfo.weight       
    FROM
             
			   Imp_ArrivalManifestULD 
			   INNER JOIN 
			   Imp_ArrivalManifestShipmentInfo on Imp_ArrivalManifestShipmentInfo.ImpArrivalManifestULDId =Imp_ArrivalManifestULD.ImpArrivalManifestULDId
			   INNER JOIN 
			   Shipment_Master on Imp_ArrivalManifestShipmentInfo.ShipmentNumber=Shipment_master.ShipmentNumber and Imp_ArrivalManifestShipmentInfo.ShipmentDate=Shipment_master.ShipmentDate
			    INNER JOIN
               Shipment_MasterSHC ON Shipment_Master.ShipmentId = Shipment_MasterSHC.ShipmentId 
				INNER JOIN
               Mst_AssociateSHCByHandlingGroup ON Shipment_MasterSHC.SpecialHandlingCode = Mst_AssociateSHCByHandlingGroup.SpecialHandlingCode 
			    INNER JOIN
               Mst_SHCHandlingGroup ON Mst_AssociateSHCByHandlingGroup.MstSHCHandlingGroupID = Mst_SHCHandlingGroup.MstSHCHandlingGroupID
			   INNER JOIN
			   Imp_InboundContainers ON  Imp_ArrivalManifestULD.ULDNumber=Imp_InboundContainers.ULDNumber and Imp_InboundContainers.HandlingMode='BREAK'
    WHERE
			 Shipment_Master.Origin !=? and Shipment_Master.Destination != ? and Imp_ArrivalManifestULD.ULDNumber is not null AND Mst_SHCHandlingGroup.SHCHandlingGroupCode IN ('BUP','BUC')
			
  ) TranshipmentIntact ON TranshipmentIntact.ShipmentId=ShipmentVerificationInfo.ShipmentId LEFT JOIN
 (
  SELECT  distinct 
       Shipment_Master.ShipmentiD,
       Imp_ArrivalManifestShipmentInfo.weight       
    FROM
             
			   Imp_ArrivalManifestULD 
			   INNER JOIN 
			   Imp_ArrivalManifestShipmentInfo on Imp_ArrivalManifestShipmentInfo.ImpArrivalManifestULDId =Imp_ArrivalManifestULD.ImpArrivalManifestULDId
			   INNER JOIN 
			   Shipment_Master on Imp_ArrivalManifestShipmentInfo.ShipmentNumber=Shipment_master.ShipmentNumber and Imp_ArrivalManifestShipmentInfo.ShipmentDate=Shipment_master.ShipmentDate
			    INNER JOIN
               Shipment_MasterSHC ON Shipment_Master.ShipmentId = Shipment_MasterSHC.ShipmentId 
				INNER JOIN
               Mst_AssociateSHCByHandlingGroup ON Shipment_MasterSHC.SpecialHandlingCode = Mst_AssociateSHCByHandlingGroup.SpecialHandlingCode 
			    INNER JOIN
               Mst_SHCHandlingGroup ON Mst_AssociateSHCByHandlingGroup.MstSHCHandlingGroupID = Mst_SHCHandlingGroup.MstSHCHandlingGroupID
			   INNER JOIN
			   Imp_InboundContainers ON  Imp_ArrivalManifestULD.ULDNumber=Imp_InboundContainers.ULDNumber and Imp_InboundContainers.HandlingMode='NOBREAK'
    WHERE
			 Shipment_Master.Origin !=? and Shipment_Master.Destination != ? and Imp_ArrivalManifestULD.ULDNumber is not null AND Mst_SHCHandlingGroup.SHCHandlingGroupCode IN ('BUP','BUC')
			
  ) TranshipmentNonIntact ON TranshipmentNonIntact.ShipmentId=ShipmentVerificationInfo.ShipmentId LEFT JOIN
(
  SELECT distinct 
              IMP_Delivery.ShipmentId,
              IMP_Delivery.DeliveryOrderNo,
              IMP_Delivery.CreatedDateTime,
              sum(SHIPMENT_FREIGHTOUT.Pieces) Pieces,
              sum(SHIPMENT_FREIGHTOUT.Weight) Weight,
              SHIPMENT_FREIGHTOUT.FlightId,
              Imp_DeliveryRequest.DeliveryRequestOrderNo,
              CUSTOMER_MASTER.CustomerCode,
              CUSTOMER_MASTER.CustomerShortName,
              POConsignee.CustomerCode Consignee
   FROM
              IMP_Delivery INNER JOIN 
              Imp_DeliveryLocalAuthorityInformation ON Imp_DeliveryLocalAuthorityInformation.ImpDeliveryId=IMP_Delivery.ImpDeliveryId
              INNER JOIN Imp_DeliveryLocalAuthorityDetails ON 
              Imp_DeliveryLocalAuthorityInformation.ImpDeliveryLocalAuthorityInformationId=Imp_DeliveryLocalAuthorityDetails.ImpDeliveryLocalAuthorityInformationId
              INNER JOIN SHIPMENT_FREIGHTOUT ON IMP_Delivery.ImpDeliveryId=SHIPMENT_FREIGHTOUT.DeliveryOrderNo LEFT JOIN
              Imp_DeliveryRequest ON SHIPMENT_FREIGHTOUT.DeliveryRequestOrderNo=Imp_DeliveryRequest.ImpDeliveryRequestId
              LEFT JOIN CUSTOMER_MASTER ON Imp_DeliveryLocalAuthorityDetails.AppointedAgentCode=CUSTOMER_MASTER.CUSTOMER_ID 
              LEFT JOIN CUSTOMER_MASTER POConsignee ON Imp_DeliveryRequest.CustomerCode=POConsignee.CustomerCode         
WHERE
              IMP_Delivery.Status='COMPLETED'
GROUP By IMP_Delivery.ShipmentId,
              IMP_Delivery.DeliveryOrderNo,
         IMP_Delivery.CreatedDateTime,SHIPMENT_FREIGHTOUT.FlightId,CUSTOMER_MASTER.CustomerCode,
              CUSTOMER_MASTER.CustomerShortName,
         Imp_DeliveryRequest.DeliveryRequestOrderNo,POConsignee.CustomerCode) Delivery ON  Delivery.ShipmentId=ShipmentVerificationInfo.ShipmentId and Delivery.flightid=ShipmentVerificationInfo.flightid
LEFT JOIN
(SELECT  distinct 
              Shipment_MasterSHC.ShipmentId,
    STRING_AGG(Shipment_MasterSHC.SpecialHandlingCode,',')shc
FROM
             Shipment_Master INNER JOIN Shipment_MasterSHC ON
                             Shipment_Master.ShipmentId = Shipment_MasterSHC.ShipmentId 
 GROUP BY Shipment_MasterSHC.ShipmentId
              ) ShipmentSHC ON ShipmentSHC.ShipmentId=ShipmentVerificationInfo.ShipmentId
WHERE 
       Shipment_Master.Destination = ? and
       Shipment_Master.ShipmentType in ('AWB') and
                 ShipmentVerificationInfo.breakdownpieces>0 and
       Shipment_Master.CancelledOn is null and
       MONTH(ShipmentVerificationInfo.DateSTA)=? and YEAR(ShipmentVerificationInfo.DateSTA)=?
       --cast(ShipmentVerificationInfo.DateSTA as datetime) between cast('01MAY2019 00:00:00' as datetime) and cast('15MAY2019 23:59:59' as datetime)
GROUP BY ShipmentVerificationInfo.CarrierCode, 
              ShipmentVerificationInfo.FlightNumber,
              Shipment_Master.ShipmentNumber,
              Shipment_Master.ShipmentiD,
              ShipmentVerificationInfo.DateATA,
              ShipmentVerificationInfo.DateSTA,
              ShipmentVerificationInfo.DateATA,
              ShipmentVerificationInfo.BreakDownPieces,
              ShipmentVerificationInfo.BreakDownWeight,
              Delivery.DeliveryOrderNo,
              Delivery.CreatedDateTime,
			  LocalIntact.weight,

            <xml-property name="queryText"><![CDATA[-- prakhar
with baseStation as (select ParameterValueChar from App_SystemParameters where ParameterCode='BASE_PORT')
select  PostingNo, PostingDate, PostedAmount, PostedQuantity, SAPCustomerCode, Customer_ID, CustomerCode,
CustomerShortName, MaterialCode, InvoiceNo, MaterialDesc,  convert(datetime, CONVERT(datetimeoffset,chargeAdviceDt) AT TIME ZONE (select ParameterValueChar from App_SystemParameters where ParameterCode='Station_timezone'))chargeAdviceDt, 
Case when ce.ReferenceType != 'RELEASE_INFO'  then ce.awbOrServiceNumber else Shipments end awbOrServiceNumber,
Terminal, ChargeableDuration, ChargeableQuantity, OriginalAmount, Amount, PayablePaymentType, 
ChargeEntryId, VerificationStatus, PaymentStatusDate, VerifiedBy, ReferenceType   , BillEntryId,       
PaymentStatus, chargeCodeName, ShipmentServiceReferenceId, EquipmentReleaseInfoId, EquipmentRequestId       , PDNumber
,EQFLTLegs.flight_id,SHC,NOG,
case when  (ce.ReferenceType = 'RELEASE_INFO' and EQFLTLegs.DateSTD is not null) then EQFLTLegs.DateSTD
              when ce.flightid is not null then 
              case when (ce.Origin = (select ParameterValueChar from baseStation))  then sfltlegs.DateSTD
                                                  when (ce.Destination = (select ParameterValueChar from baseStation))  then sfltlegs.DateSTA
                                    end
              when (ce.ReferenceType='Shipment' and ce.Origin = (select ParameterValueChar from baseStation))  then
              (select top 1 Flt_OperativeFlight_Segments.DateSTD 
                     from Exp_ManifestShipmentInfo
                                  inner join  Exp_Manifest on Exp_Manifest.ManifestId=Exp_ManifestShipmentInfo.ManifestId
                                  inner join Flt_OperativeFlight on Flt_OperativeFlight.Flight_ID=Exp_Manifest.Flightid
                                  inner join Flt_OperativeFlight_Segments on Flt_OperativeFlight_Segments.FlightSegmentId=Exp_Manifest.FlightSegmentId          
           where Exp_ManifestShipmentInfo.ShipmentId = ce.shipmentservicereferenceid and ce.ReferenceType='Shipment'
              and FlightBoardPoint=(select ParameterValueChar from baseStation)
                       order by Flt_OperativeFlight_Segments.DateSTD
                     ) 
              when (ce.ReferenceType='Shipment' and ce.Destination = (select ParameterValueChar from baseStation))  then
              (
                     select   top 1 Flt_OperativeFlight_Legs.DateSTA  
                           from  Imp_ShipmentVerification
                           inner join Flt_OperativeFlight on Flt_OperativeFlight.Flight_ID=Imp_ShipmentVerification.FlightId
                           inner join Flt_OperativeFlight_Legs on Flt_OperativeFlight_Legs.Flight_ID= Flt_OperativeFlight.Flight_ID
                           where Imp_ShipmentVerification.ShipmentId = ce.shipmentservicereferenceid and ce.ReferenceType='Shipment'
                     and FlightOffPoint=(select ParameterValueChar from baseStation)
                     order by Flt_OperativeFlight_Legs.DateSTA 
              ) 
              end STD ,
case when  (ce.ReferenceType = 'RELEASE_INFO' and isnull(EQFLT.FlightKey, RI.CarrierCode) is not null)  then isnull(EQFLT.FlightKey, RI.CarrierCode)
              when ce.flightid is not null then sflt.FlightKey
              when (ce.ReferenceType='Shipment' and ce.Origin = (select ParameterValueChar from baseStation))  then
              (select top 1 Flt_OperativeFlight.FlightKey 
                     from Exp_ManifestShipmentInfo
                                  inner join  Exp_Manifest on Exp_Manifest.ManifestId=Exp_ManifestShipmentInfo.ManifestId
                                  inner join Flt_OperativeFlight on Flt_OperativeFlight.Flight_ID=Exp_Manifest.Flightid
                                  inner join Flt_OperativeFlight_Segments on Flt_OperativeFlight_Segments.FlightSegmentId=Exp_Manifest.FlightSegmentId          
           where Exp_ManifestShipmentInfo.ShipmentId = ce.shipmentservicereferenceid and ce.ReferenceType='Shipment'
              and FlightBoardPoint=(select ParameterValueChar from baseStation)
                       order by Flt_OperativeFlight_Segments.DateSTD
                     ) 
              when (ce.ReferenceType='Shipment' and ce.Destination = (select ParameterValueChar from baseStation))  then
              (
                     select   top 1 Flt_OperativeFlight.FlightKey 
                           from  Imp_ShipmentVerification
                           inner join Flt_OperativeFlight on Flt_OperativeFlight.Flight_ID=Imp_ShipmentVerification.FlightId
                           inner join Flt_OperativeFlight_Legs on Flt_OperativeFlight_Legs.Flight_ID= Flt_OperativeFlight.Flight_ID
                           where Imp_ShipmentVerification.ShipmentId = ce.shipmentservicereferenceid and ce.ReferenceType='Shipment'
                     and FlightOffPoint=(select ParameterValueChar from baseStation)
                     order by Flt_OperativeFlight_Legs.DateSTA
              ) 
              end FlightKey 
             ,(select convert(datetime,SWITCHOFFSET(converT(datetimeoffset,ri.CreatedDateTime),(SELECT current_utc_offset 
                 FROM   sys.time_zone_info 
                 WHERE  NAME = 
                                (select ParameterValueChar 
                                       from App_SystemParameters 
                                       where
                                       upper(ParameterCode)=upper('Station_timezone')))))) as EquipmentReleaseDateTime
             
 from 
   (  select  
   
   be.BillingNo PostingNo, 
   UPPER(FORMAT(convert(datetime, CONVERT(datetimeoffset, be.PostingDate) AT TIME ZONE 
   (select ParameterValueChar from App_SystemParameters where ParameterCode='Station_timezone')), 'ddMMMyyyy HH:mm')) PostingDate, 
   be.ApportionedAmount PostedAmount, be.ApportionedQuantity PostedQuantity,
   bc.FinanceSystemIdentificationNumber SAPCustomerCode, cm.Customer_ID, cm.CustomerCode, cm.CustomerShortName, be.FinSysChargeCode MaterialCode,
   be.FinSysInvoiceNumber InvoiceNo, fscc.FinSysChargeDesc MaterialDesc,    ce.CreatedDateTime chargeAdviceDt,
case when ce.ReferenceType != 'RELEASE_INFO' then
   coalesce(bsrq.ServiceRequestNo, sm.ShipmentNumber) 
  end   
   as awbOrServiceNumber,
   ce.handlingarea Terminal,   ce.ChargeableDuration ChargeableDuration,   ce.ChargeableQuantity ChargeableQuantity,   ce.OriginalAmount,   ce.Amount,
   ce.PayablePaymentType,   ce.ChargeEntryId,   ce.VerificationStatus,   ce.PaymentStatusDate,   ce.VerifiedBy,   ce.ReferenceType,   be.BillEntryId,
   case
              when ce.PaymentStatus = 'Void' then 'Void'
              when be.PostingStatus = 0 then 'Pending'
              when be.PostingStatus = 1 and FinSysInvoiceNumber is null then 'Posted'
              when be.PostingStatus = 1 and FinSysInvoiceNumber is not null then 'Invoiced'
       else 'Pending' end PaymentStatus,
       case 
              when cmf.ComboDescription is null or cmf.ComboDescription = '' then cc.Description
              when cmf.ComboDescription is not null then cc.Description+' ('+cmf.ComboDescription+')'
              else cc.Description end as chargeCodeName
                     , ce.FlightId, ce.ShipmentServiceReferenceId , sm.Origin, sm.Destination,
                     (select string_agg(SpecialHandlingCode,',')  from Shipment_MasterSHC where ShipmentId=ce.ShipmentServiceReferenceId and ce.ReferenceType='Shipment') SHC,
sm.NatureOfGoodsDescription as NOG     
   from 
   Billing_ChargeEntry ce
   inner join Billing_ChargeCode cc on cc.BillingChargeCodeId=ce.ChargecodeId
   inner join Billing_ChargeModelFactor  cmf on cmf.BillingChargecodeId=cc.BillingChargecodeId and cmf.ChargeModelFactorId=ce.SubGroupId
   left join Billing_ChargeEntryBillPaid cebp on cebp.ChargeEntryId=ce.ChargeEntryId
   left join Billing_BillEntry be on be.BillEntryId=cebp.BillEntryId
   left join Customer_Master cm on cm.Customer_ID = ce.CustomerId
   left join Shipment_Master sm on sm.ShipmentId=ce.ShipmentServiceReferenceId and ce.ReferenceType='Shipment'
   left join Billing_ServiceRequest  bsrq on bsrq.ServiceRequestId=ce.ShipmentServiceReferenceId and    ce.ReferenceType = 'Service'
   left join Billing_FinanceSystemChargeCode fscc on fscc.FinSysChargeCode= be.FinSysChargeCode
   left join Billing_CustomerInfo bc on bc.CustomerId=cm.Customer_ID
   
where 
1=? and

ce.OriginalAmount != 0  and ce.CustomerId is not null and ce.ChargingStatus = 'Closed' and PayablePaymentType = 'Bill/Collect'

--replacement --(Dont remove this line used for replacement for filter in script)

) ce left join (
              select RRD.EquipmentReleaseInfoId, ER.EquipmentRequestId, ER.CarrierCode, ER.FlightId,  string_agg( ShipmentNumber, ',') Shipments, ERI.PDNumber,
              ER.RequestFor, ERI.CreatedDateTime 
              from Equipment_Release_Info ERI
              inner join Equipment_Request_Release_Details RRD on ERI.EquipmentReleaseInfoId = RRD.EquipmentReleaseInfoId
              inner join Equipment_Request ER on ER.EquipmentRequestId = RRD.EquipmentRequestId  
              left join Equipment_Request_Shipments ERS on ERS.EquipmentRequestId = RRD.EquipmentRequestId
              Group by RRD.EquipmentReleaseInfoId, ER.EquipmentRequestId, ER.CarrierCode, ER.FlightId,   ERI.PDNumber, ER.RequestFor,ERI.CreatedDateTime 
   ) ri on ri.EquipmentReleaseInfoId = ce.ShipmentServiceReferenceId and ce.ReferenceType = 'RELEASE_INFO' 
       left join Flt_OperativeFlight EQFLT on (EQFLT.Flight_ID=ri.FlightId and ce.ReferenceType = 'RELEASE_INFO')
       left join Flt_OperativeFlight_Legs EQFLTLegs on EQFLTLegs.Flight_ID= EQFLT.Flight_ID
              and (
              (RequestFor= 'Export'  and (EQFLTLegs.FlightBoardPoint=(select ParameterValueChar from baseStation)))
              or
              (RequestFor= 'Import'  and (EQFLTLegs.FlightOffPoint=(select ParameterValueChar from baseStation)))
              )
       left join Flt_OperativeFlight SFLT on (SFLT.Flight_ID=ce.FlightId and ce.ReferenceType != 'RELEASE_INFO')
       left join Flt_OperativeFlight_Legs SFLTLegs on SFLTLegs.Flight_ID= SFLT.Flight_ID        
              and 
                        (
                        (ce.Origin = (select ParameterValueChar from baseStation) and  SFLTLegs.FlightBoardPoint=(select ParameterValueChar from baseStation))
                        or
                        (ce.Destination = (select ParameterValueChar from baseStation) and  SFLTLegs.FlightOffPoint=(select ParameterValueChar from baseStation))
                        )
where (ce.ReferenceType != 'RELEASE_INFO' or (ri.EquipmentReleaseInfoId is not null and ri.PDNumber != ''))                        
order by ce.chargeCodeName, ce.CustomerCode
]]></xml-property>

            <method name="beforeOpen"><![CDATA[var QryCond = '';
var customerCode = params["customerCode"].value;
var chargeCode = params["chargeCode"].value;
var chargeFactorId = params["chargeFactorId"].value;
var awbOrServiceNumber = params["awbOrServiceNumber"].value;
var transactionDateFrom = params["transactionDateFrom"].value;
var transactionDateTo = params["transactionDateTo"].value;
var status = params["status"].value;
var statusDateFrom = params["statusDateFrom"].value;
var statusDateTo = params["statusDateTo"].value;
var verificationStatus = params["verificationStatus"].value;
var handlingArea= params["handlingArea"].value;

if(customerCode != null &amp;&amp; customerCode != ""){
	QryCond  = QryCond  + " and ce.CustomerId = '"+customerCode+"'";
} 

if(chargeCode != null &amp;&amp; chargeCode != ""){
	QryCond  = QryCond  + " and ce.ChargeCodeId = '"+chargeCode+"'";
	}

if(chargeFactorId != null &amp;&amp; chargeFactorId != ""){
	QryCond  = QryCond  + " and cmf.ChargeModelFactorId = '"+chargeFactorId+"'";
}

if(awbOrServiceNumber != null &amp;&amp; awbOrServiceNumber != ""){
	QryCond  = QryCond  + " and (bsrq.ServiceRequestNo like '"+awbOrServiceNumber+"' + '%' or sm.ShipmentNumber like '"+awbOrServiceNumber+"' + '%')";
}

if(transactionDateFrom != null &amp;&amp; transactionDateFrom != ""){
	QryCond  = QryCond  + " and convert(datetime, CONVERT(datetimeoffset, ce.CreatedDateTime) AT TIME ZONE (select ParameterValueChar from App_SystemParameters where ParameterCode='Station_timezone')) >= '"+transactionDateFrom+"'";
}

if(transactionDateTo != null &amp;&amp; transactionDateTo != ""){
	QryCond  = QryCond  + " and convert(datetime, CONVERT(datetimeoffset, ce.CreatedDateTime) AT TIME ZONE (select ParameterValueChar from App_SystemParameters where ParameterCode='Station_timezone')) <= '"+transactionDateTo+"'";
}
if(status != null &amp;&amp; status != "" &amp;&amp; status == "Void"){
	QryCond  = QryCond  + " and ce.PaymentStatus = 'Void'";
}

if(status != null &amp;&amp; status != "" &amp;&amp; status == "Pending"){
	QryCond  = QryCond  + " and (be.PostingStatus is null or be.PostingStatus = 0) and ce.PaymentStatus != 'Void'";
}
if(status != null &amp;&amp; status != "" &amp;&amp; status == "Invoiced"){
	QryCond  = QryCond  + "and be.PostingStatus = 1 and FinSysInvoiceNumber is not null";
}
if(status != null &amp;&amp; status != "" &amp;&amp; status == "Posted"){
	QryCond  = QryCond  + " and be.PostingStatus = 1 and FinSysInvoiceNumber is null";
}
if (statusDateFrom != null &amp;&amp; status != null &amp;&amp; status !="" &amp;&amp; status == "Void"){
	QryCond  = QryCond  + " and convert(datetime, CONVERT(datetimeoffset, ce.VerificationStatusDate) AT TIME ZONE (select ParameterValueChar from App_SystemParameters where ParameterCode='Station_timezone')) >= '"+statusDateFrom+"'";
}
if(statusDateFrom != null &amp;&amp; status != null &amp;&amp; status !="" &amp;&amp; status == "Posted"){
	QryCond  = QryCond  + " and convert(datetime, CONVERT(datetimeoffset, be.PostingDate) AT TIME ZONE (select ParameterValueChar from App_SystemParameters where ParameterCode='Station_timezone')) >= '"+statusDateFrom+"'";
}
if(statusDateFrom != null &amp;&amp; status != null &amp;&amp; status !="" &amp;&amp; status == "Invoiced"){
	QryCond  = QryCond  + " and convert(datetime, CONVERT(datetimeoffset, be.FinSysInvoiceDate) AT TIME ZONE (select ParameterValueChar from App_SystemParameters where ParameterCode='Station_timezone')) >= '"+statusDateFrom+"'";
}
if(statusDateTo != null &amp;&amp; status != null &amp;&amp; status !="" &amp;&amp; status == "Void"){
	QryCond  = QryCond + "and convert(datetime, CONVERT(datetimeoffset, ce.VerificationStatusDate) AT TIME ZONE (select ParameterValueChar from App_SystemParameters where ParameterCode='Station_timezone')) <= '"+statusDateTo+"'"
}
if(statusDateTo != null &amp;&amp; status != null &amp;&amp; status !="" &amp;&amp; status == "Posted"){
	QryCond = QryCond  + " and convert(datetime, CONVERT(datetimeoffset, be.PostingDate) AT TIME ZONE (select ParameterValueChar from App_SystemParameters where ParameterCode='Station_timezone')) <= '"+statusDateTo+"'";
}
if(statusDateTo != null &amp;&amp; status != null &amp;&amp; status !="" &amp;&amp; status == "Invoiced"){
	QryCond  = QryCond  + " and convert(datetime, CONVERT(datetimeoffset, be.FinSysInvoiceDate) AT TIME ZONE (select ParameterValueChar from App_SystemParameters where ParameterCode='Station_timezone')) <= '"+statusDateTo+"'";
}
if(verificationStatus != null &amp;&amp; verificationStatus != ""){
	QryCond  = QryCond  + " and ce.VerificationStatus =  '"+verificationStatus+"'";
}
if(handlingArea != null &amp;&amp; handlingArea != ""){
	QryCond  = QryCond  + " and ce.handlingarea =  '"+handlingArea+"'";
}

this.queryText = this.queryText.replace('--replacement', QryCond);

]]></method>

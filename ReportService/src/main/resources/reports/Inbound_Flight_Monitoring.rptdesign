            <method name="beforeOpen"><![CDATA[var terminals = params["terminals"].value;
var fromDate = params["fromDate"].value;
var toDate = params["toDate"].value;
var carrierGroup = params["carrierGroup"].value;
var acType = params["acType"].value;
var carrier = params["carrier"].value;
var flight = params["flight"].value;
var date = params["date"].value;


if(terminals != null &amp;&amp; terminals != "")
{
this.queryText = this.queryText + "and exists(select 1 from Flt_OperativeFlight_HandlingArea where Flt_OperativeFlight_HandlingArea.FlightId = Flt_OperativeFlight.Flight_ID and Flt_OperativeFlight_HandlingArea.TerminalCode='"+terminals+"')"
}

if(fromDate != null  &amp;&amp; toDate != null  )
{
this.queryText = this.queryText + "and ((Flt_OperativeFlight_Legs.DateSTA BETWEEN '"+fromDate+"'  and '"+toDate+"') OR  (Flt_OperativeFlight_Legs.DateETA BETWEEN '"+fromDate+"'  and '"+toDate+"') OR (Flt_OperativeFlight_Legs.DateATA BETWEEN '"+fromDate+"' and '"+toDate+"' ))"
}

if(carrierGroup != null &amp;&amp; carrierGroup != "")
{
this.queryText = this.queryText + "and Flt_OperativeFlight.CarrierCode in (select Mst_AssociateCarrierWithGroup.CarrierCode from  Mst_CarrierGroup inner join Mst_AssociateCarrierWithGroup on Mst_CarrierGroup.Mst_CarrierGroup_ID = Mst_AssociateCarrierWithGroup.Mst_CarrierGroup_ID where Mst_CarrierGroup.CarrierGroupCode = (SELECT distinct CarrierGroupCode FROM Mst_CarrierGroup where Mst_CarrierGroup_ID  = '"+carrierGroup+"'))"
}
if(acType != null &amp;&amp; acType != "")
{
this.queryText = this.queryText + "and Flt_OperativeFlight_Legs.AircraftType = '"+fromDate+"'"
}
if(carrier != null &amp;&amp; carrier != "")
{
this.queryText = this.queryText + "and Flt_OperativeFlight.CarrierCode = '"+carrier+"'"
}
if(flight != null &amp;&amp; flight != "")
{
this.queryText = this.queryText + "and Flt_OperativeFlight.FlightKey = '"+flight+"'"
}
if(date != null &amp;&amp; date != "")
{
this.queryText = this.queryText + "and cast(Flt_OperativeFlight_Legs.DateSTA as date) = CAST('"+date+"' AS DATE)"
}

this.queryText = this.queryText + "order by  Flt_OperativeFlight_Legs.DateSTA, Flt_OperativeFlight_Legs.DateETA"




]]></method>

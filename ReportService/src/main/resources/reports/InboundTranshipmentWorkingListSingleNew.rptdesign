                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">32</property>
                    <property name="name">ERR</property>
                    <property name="nativeName">ERR</property>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">33</property>
                    <property name="name">FTT</property>
                    <property name="nativeName">FTT</property>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">34</property>
                    <property name="name">Finalized</property>
                    <property name="nativeName">Finalized</property>
                    <property name="dataType">integer</property>
                </structure>
                <structure>
                    <property name="position">35</property>
                    <property name="name">Assigned</property>
                    <property name="nativeName">Assigned</property>
                    <property name="dataType">integer</property>
                </structure>
                <structure>
                    <property name="position">36</property>
                    <property name="name">ShipmentFinalized</property>
                    <property name="nativeName">ShipmentFinalized</property>
                    <property name="dataType">string</property>
                </structure>
                <structure>
                    <property name="position">37</property>
                    <property name="name">UldFinalized</property>
                    <property name="nativeName">UldFinalized</property>
                    <property name="dataType">string</property>
                </structure>
            </list-property>
            <xml-property name="queryText"><![CDATA[with TTWLBookingInfo AS(
	select
		Flt_OperativeFlight_Segments.DateSTD as DepartureDateTime,
		Flt_OperativeFlight.Flight_ID as DepartureFlightId,
		Flt_OperativeFlight.FlightKey as DepartureFlightKey,
		upper(format(Flt_OperativeFlight_Segments.DateSTD, 'ddMMM')) as DepartureFlightDate,
		upper(format(Flt_OperativeFlight_Segments.DateSTD, 'HHmm')) as DepartureFlightTime,
		Flt_OperativeFlight_Legs.FlightOffPoint as DepartureFlightOffPoint,
		Flt_OperativeFlight_Segments.FlightSegmentId as DepartureFlightSegmentId,
		Flt_OperativeFlight_Segments.FlightSegmentOrder as DepartureFlightSegmentOrder,
		Flt_OperativeFlight.DateSTD as DepartureFlightSTD,
		Flt_OperativeFlight_Segments.FlightBoardPoint + '-' + Flt_OperativeFlight_Segments.FlightOffPoint as DepartureFlightSegment,
		case when (Exp_FlightEvents.FlightDepartedOn is not null) then 'DEP'
			 when (Exp_FlightEvents.ManifestCompletedAt is not null) then 'MAN'
			 when (Exp_FlightEvents.DLSCompletedAt is not null) then 'DLS'
			 else null end DepartureFlightStatus,
		inwardFlight.Flight_ID as ArrivalFlightId,
		inwardFlight.FlightKey as ArrivalFlightKey,
		upper(format(inwardFlightSegments.DateSTA, 'ddMMM')) as ArrivaleFlightDate,
		upper(format(inwardFlightSegments.DateSTA, 'HHmm')) as ArrivalFlightTime,
		inwardFlightSegments.FlightBoardPoint as ArrivalFlightOffPoint,
		inwardFlightSegments.DateSTA as ArrivalFlightSTA,
		inwardFlightSegments.FlightSegmentId as ArrivalFlightSegmentId,
		inwardFlightSegments.FlightSegmentOrder as ArrivaleFlightSegmentOrder,
		inwardFlightSegments.FlightBoardPoint + '-' + inwardFlightSegments.FlightOffPoint as ArrivalFlightSegment,
		substring(Exp_ShipmentBooking.ShipmentNumber, 0, 4) + '-'+ substring(Exp_ShipmentBooking.ShipmentNumber, 4, 12) as DisplayShipmentNumber,
		Exp_ShipmentBooking.ShipmentNumber,
		Exp_ShipmentBooking.ShipmentDate,
		Exp_ShipmentBooking.Origin,
		Exp_ShipmentBooking.Destination,
		Exp_ShipmentBooking.NatureOfGoodsDescription,
		(Exp_ShipmentPartBookingDetail.PartPieces) as BookedPieces,
		(Exp_ShipmentPartBookingDetail.PartWeight) as BookedWeight,
		case 
			when (select count(1) from Shipment_FreightWayBill 
					where Shipment_FreightWayBill.AwbNumber = Exp_ShipmentBooking.ShipmentNumber and 
							Shipment_FreightWayBill.AwbDate = Exp_ShipmentBooking.ShipmentDate) > 0 then
				'#'
			else '' end as FWB,
		(select
				TOP 1 Exp_ShipmentFlightBookingDetailSHC.SpecialHandlingCode
			from
				Exp_ShipmentFlightBookingDetailSHC inner join
				Mst_SpecialHandlingCode
					on Exp_ShipmentFlightBookingDetailSHC.SpecialHandlingCode = Mst_SpecialHandlingCode.SpecialHandlingCode
			where
				Exp_ShipmentFlightBookingDetailSHC.FlightBookingId = Exp_ShipmentFlightBookingDetail.FlightBookingId
			order by
				Mst_SpecialHandlingCode.SpecialHandlingPriority
		) as TOPPrioritySHC,
		'BULK' as ULDNumber,
		Exp_ShipmentFlightBookingDetail.Source
	from
		Flt_OperativeFlight inner join 
		Exp_FlightEvents
			on Flt_OperativeFlight.Flight_ID = Exp_FlightEvents.FlightId inner join
		Flt_OperativeFlight_Legs
			on Flt_OperativeFlight.Flight_ID = Flt_OperativeFlight_Legs.Flight_ID and
				Flt_OperativeFlight_Legs.FlightBoardPoint = ? inner join
		Flt_OperativeFlight_Segments
			on Flt_OperativeFlight.Flight_ID = Flt_OperativeFlight_Segments.Flight_ID and
				Flt_OperativeFlight_Segments.FlightBoardPoint = ? inner join
		Exp_ShipmentFlightBookingDetail
			on Flt_OperativeFlight_Segments.FlightSegmentId = Exp_ShipmentFlightBookingDetail.FlightSegmentId inner join
		Exp_ShipmentFlightPartDetail
			on Exp_ShipmentFlightBookingDetail.FlightBookingId = Exp_ShipmentFlightPartDetail.FlightBookingId inner join
		Exp_ShipmentPartBookingDetail
			on Exp_ShipmentFlightPartDetail.PartBookingId = Exp_ShipmentPartBookingDetail.PartBookingId inner join
		Exp_ShipmentBooking
			on Exp_ShipmentPartBookingDetail.BookingId = Exp_ShipmentBooking.BookingId inner join
		Exp_ShipmentFlightBookingDetail inwardFlightBooking
			on Exp_ShipmentBooking.BookingId = inwardFlightBooking.BookingId inner join
		Exp_ShipmentFlightPartDetail inwardFlightPart
			on inwardFlightBooking.FlightBookingId = inwardFlightPart.FlightBookingId and
				Exp_ShipmentPartBookingDetail.PartBookingId = inwardFlightPart.PartBookingId inner join
		Exp_ShipmentPartBookingDetail inwardPart
			on inwardFlightPart.PartBookingId = inwardPart.PartBookingId inner join
		Flt_OperativeFlight_Segments inwardFlightSegments
			on inwardFlightBooking.FlightId = inwardFlightSegments.Flight_ID and
				inwardFlightBooking.FlightSegmentId = inwardFlightSegments.FlightSegmentId inner join
		Flt_OperativeFlight inwardFlight
			on inwardFlightSegments.Flight_ID = inwardFlight.Flight_ID inner join
		Flt_OperativeFlight_Legs inwardFlightLeg
			on inwardFlight.Flight_ID = inwardFlightLeg.Flight_ID and
				inwardFlightLeg.FlightOffPoint = ?
	where                        
		inwardFlight.FlightKey = ? and
		cast(inwardFlightSegments.DateSTA as date) = cast(? as date) and
		Exp_ShipmentPartBookingDetail.ThroughTransitFlag = 1 and
		Exp_ShipmentBooking.Origin NOT IN (?, ?) and 
		Exp_ShipmentBooking.Destination NOT IN (?, ?) and
		isnull(Exp_ShipmentFlightBookingDetail.OutwardBookingFlag, 0) = 1 and
		exists(
			select 
				null
			from
				Transhipment_TransferTypes 
			where
				Transhipment_TransferTypes.Code = Exp_ShipmentPartBookingDetail.TransferType and
				Transhipment_TransferTypes.TransferType = 'TT'
		)	
)
select
	*
from(
	select
		TTWLBookingInfo.DepartureDateTime,
		TTWLBookingInfo.DepartureFlightKey,
		TTWLBookingInfo.DepartureFlightDate,
		TTWLBookingInfo.DepartureFlightTime,
		TTWLBookingInfo.DepartureFlightOffPoint,
		TTWLBookingInfo.DepartureFlightSegment,
		TTWLBookingInfo.DepartureFlightSegmentOrder,
		TTWLBookingInfo.DepartureFlightSTD,
		TTWLBookingInfo.DepartureFlightStatus,
		TTWLBookingInfo.ArrivalFlightKey,
		TTWLBookingInfo.ArrivaleFlightDate,
		TTWLBookingInfo.ArrivalFlightTime,
		TTWLBookingInfo.ArrivalFlightOffPoint,
		TTWLBookingInfo.ArrivalFlightSegment,
		TTWLBookingInfo.ArrivaleFlightSegmentOrder,
		TTWLBookingInfo.ArrivalFlightSTA,
		TTWLBookingInfo.DisplayShipmentNumber,
		TTWLBookingInfo.Origin,
		TTWLBookingInfo.Destination,
		TTWLBookingInfo.NatureOfGoodsDescription,
		TTWLBookingInfo.BookedPieces as TTWLPieces,
		TTWLBookingInfo.BookedWeight as TTWLWeigtht,
		TTWLBookingInfo.FWB,
		TTWLBookingInfo.TOPPrioritySHC,
		null TTWLLoadedPieces,
		null TTWLLoadedWeight,
		TTWLBookingInfo.ULDNumber,	
		null as GrossWeight,	
		null as ContourCode,
		null as ULDGrossWeight,
		'N' Ready,
		null ERR,
		case when TTWLBookingInfo.Source = 'FFR' then 'Y' else null end as FTT,
		null Finalized,
		null Assigned,
		null ShipmentFinalized,
		null UldFinalized
	from
		TTWLBookingInfo
	where
		(not exists(
			select
				null
			from
				Imp_ArrivalManifestByFlight inner join
				Imp_ArrivalManifestBySegment
					on Imp_ArrivalManifestByFlight.ImpArrivalManifestByFlightId = Imp_ArrivalManifestBySegment.ImpArrivalManifestByFlightId inner join
				Imp_ArrivalManifestULD
					on Imp_ArrivalManifestBySegment.ImpArrivalManifestBySegmentId = Imp_ArrivalManifestULD.ImpArrivalManifestBySegmentId inner join
				Imp_ArrivalManifestShipmentInfo
					on Imp_ArrivalManifestULD.ImpArrivalManifestULDId = Imp_ArrivalManifestShipmentInfo.ImpArrivalManifestULDId
			where
				Imp_ArrivalManifestByFlight.FlightId = TTWLBookingInfo.ArrivalFlightId and
				Imp_ArrivalManifestShipmentInfo.ShipmentNumber = TTWLBookingInfo.ShipmentNumber and
				Imp_ArrivalManifestShipmentInfo.ShipmentDate = TTWLBookingInfo.ShipmentDate and
				Imp_ArrivalManifestShipmentInfo.BookingFlightId = TTWLBookingInfo.DepartureFlightId and
				Imp_ArrivalManifestShipmentInfo.FlightSegmentId = TTWLBookingInfo.DepartureFlightSegmentId and
				exists(
					select 
						null
					from
						Transhipment_TransferTypes 
					where
						Transhipment_TransferTypes.Code = Imp_ArrivalManifestShipmentInfo.TransferType and
						Transhipment_TransferTypes.TransferType = 'TT'
				)
		) and
		not exists(
			select
				null
			from
				Exp_AssignedULDTrolleyToFlight inner join
				Exp_LoadedShipmentInfo
					on Exp_AssignedULDTrolleyToFlight.AssUldTrolleyId = Exp_LoadedShipmentInfo.AssUldTrolleyId inner join
				Shipment_Master
					on Exp_LoadedShipmentInfo.ShipmentId = Shipment_Master.ShipmentId
			where
				Exp_AssignedULDTrolleyToFlight.FlightId = TTWLBookingInfo.DepartureFlightId and
				Shipment_Master.ShipmentNumber = TTWLBookingInfo.ShipmentNumber and
				Shipment_Master.ShipmentDate = TTWLBookingInfo.ShipmentDate
		))
	union
	select
		TTWLBookingInfo.DepartureDateTime,
		TTWLBookingInfo.DepartureFlightKey,
		TTWLBookingInfo.DepartureFlightDate,
		TTWLBookingInfo.DepartureFlightTime,
		TTWLBookingInfo.DepartureFlightOffPoint,
		TTWLBookingInfo.DepartureFlightSegment,
		TTWLBookingInfo.DepartureFlightSegmentOrder,
		TTWLBookingInfo.DepartureFlightSTD,
		TTWLBookingInfo.DepartureFlightStatus,
		TTWLBookingInfo.ArrivalFlightKey,
		TTWLBookingInfo.ArrivaleFlightDate,
		TTWLBookingInfo.ArrivalFlightTime,
		TTWLBookingInfo.ArrivalFlightOffPoint,
		TTWLBookingInfo.ArrivalFlightSegment,
		TTWLBookingInfo.ArrivaleFlightSegmentOrder,
		TTWLBookingInfo.ArrivalFlightSTA,
		TTWLBookingInfo.DisplayShipmentNumber,
		Imp_ArrivalManifestShipmentInfo.Origin,
		Imp_ArrivalManifestShipmentInfo.Destination,
		Imp_ArrivalManifestShipmentInfo.NatureOfGoodsDescription,
		Imp_ArrivalManifestShipmentInfo.Piece as TTWLPieces,
		Imp_ArrivalManifestShipmentInfo.Weight as TTWLWeigtht,
		TTWLBookingInfo.FWB,
		isnull((select
						TOP 1 Imp_ArrivalManifestByShipmentSHC.SpecialHandlingCode
					from
						Imp_ArrivalManifestByShipmentSHC inner join
						Mst_SpecialHandlingCode
							on Imp_ArrivalManifestByShipmentSHC.SpecialHandlingCode = Mst_SpecialHandlingCode.SpecialHandlingCode
					where
						Imp_ArrivalManifestByShipmentSHC.ImpArrivalManifestShipmentInfoId = Imp_ArrivalManifestShipmentInfo.ImpArrivalManifestShipmentInfoId
					order by
						Mst_SpecialHandlingCode.SpecialHandlingPriority
				), TTWLBookingInfo.TOPPrioritySHC) as TOPPrioritySHC,
		null TTWLLoadedPieces,
		null TTWLLoadedWeight,
		isnull(Imp_ArrivalManifestULD.ULDNumber, 'BULK') ULDNumber,	
		'GK' as GrossWeight,	
		Imp_ContainerPalletMessageULD.ContourCode as ContourCode,
		Imp_ContainerPalletMessageULD.Weight as ULDGrossWeight,
		'N' Ready,
		null ERR,
		case when TTWLBookingInfo.Source = 'FFR' then 'Y' else null end as FTT,
		null Finalized,
		null Assigned,
        null ShipmentFinalized,
		null UldFinalized
	from
		Imp_ArrivalManifestByFlight inner join
		Imp_ArrivalManifestBySegment
			on Imp_ArrivalManifestByFlight.ImpArrivalManifestByFlightId = Imp_ArrivalManifestBySegment.ImpArrivalManifestByFlightId inner join
		Imp_ArrivalManifestULD
			on Imp_ArrivalManifestBySegment.ImpArrivalManifestBySegmentId = Imp_ArrivalManifestULD.ImpArrivalManifestBySegmentId inner join
		Imp_ArrivalManifestShipmentInfo
			on Imp_ArrivalManifestULD.ImpArrivalManifestULDId = Imp_ArrivalManifestShipmentInfo.ImpArrivalManifestULDId inner join
		TTWLBookingInfo
			on Imp_ArrivalManifestByFlight.FlightId = TTWLBookingInfo.ArrivalFlightId and
				Imp_ArrivalManifestBySegment.FlightSegmentId = TTWLBookingInfo.ArrivalFlightSegmentId and
				Imp_ArrivalManifestShipmentInfo.ShipmentNumber = TTWLBookingInfo.ShipmentNumber and
				Imp_ArrivalManifestShipmentInfo.ShipmentDate = TTWLBookingInfo.ShipmentDate and
				Imp_ArrivalManifestShipmentInfo.BookingFlightId = TTWLBookingInfo.DepartureFlightId and
				Imp_ArrivalManifestShipmentInfo.FlightSegmentId = TTWLBookingInfo.DepartureFlightSegmentId left join
		Imp_ContainerPalletMessageByFlight
			on Imp_ArrivalManifestByFlight.FlightId = Imp_ContainerPalletMessageByFlight.FlightId left join
		Imp_ContainerPalletMessageBySegment
			on Imp_ContainerPalletMessageByFlight.ImpContainerPalletMessageByFlightId = 
					Imp_ContainerPalletMessageBySegment.ImpContainerPalletMessageByFlightId left join
		Imp_ContainerPalletMessageULD
			on Imp_ContainerPalletMessageBySegment.ImpContainerPalletMessageBySegmentId = 
					Imp_ContainerPalletMessageULD.ImpContainerPalletMessageBySegmentId and
				Imp_ArrivalManifestULD.ULDNumber = Imp_ContainerPalletMessageULD.ULDNumber
	where
		exists(
				select 
					null
				from
					Transhipment_TransferTypes 
				where
					Transhipment_TransferTypes.Code = Imp_ArrivalManifestShipmentInfo.TransferType and
					Transhipment_TransferTypes.TransferType = 'TT'
			) and
			not exists(
				select
					null
				from
					Exp_AssignedULDTrolleyToFlight inner join
					Exp_LoadedShipmentInfo
						on Exp_AssignedULDTrolleyToFlight.AssUldTrolleyId = Exp_LoadedShipmentInfo.AssUldTrolleyId inner join
					Shipment_Master
						on Exp_LoadedShipmentInfo.ShipmentId = Shipment_Master.ShipmentId
				where
					Exp_AssignedULDTrolleyToFlight.FlightId = Imp_ArrivalManifestShipmentInfo.BookingFlightId and
					Shipment_Master.ShipmentNumber = Imp_ArrivalManifestShipmentInfo.ShipmentNumber and
					Shipment_Master.ShipmentDate = Imp_ArrivalManifestShipmentInfo.ShipmentDate and
					Exp_AssignedULDTrolleyToFlight.ULDTrolleyNumber = isnull(Imp_ArrivalManifestULD.ULDNumber, Exp_AssignedULDTrolleyToFlight.ULDTrolleyNumber)
			)
	group by
		TTWLBookingInfo.DepartureDateTime,
		TTWLBookingInfo.DepartureFlightKey,
		TTWLBookingInfo.DepartureFlightDate,
		TTWLBookingInfo.DepartureFlightTime,
		TTWLBookingInfo.DepartureFlightOffPoint,
		TTWLBookingInfo.DepartureFlightSegment,
		TTWLBookingInfo.DepartureFlightSegmentOrder,
		TTWLBookingInfo.DepartureFlightSTD,
		TTWLBookingInfo.DepartureFlightStatus,
		TTWLBookingInfo.ArrivalFlightKey,
		TTWLBookingInfo.ArrivaleFlightDate,
		TTWLBookingInfo.ArrivalFlightTime,
		TTWLBookingInfo.ArrivalFlightOffPoint,
		TTWLBookingInfo.ArrivalFlightSegment,
		TTWLBookingInfo.ArrivaleFlightSegmentOrder,
		TTWLBookingInfo.ArrivalFlightSTA,
		TTWLBookingInfo.DisplayShipmentNumber,
		Imp_ArrivalManifestShipmentInfo.Origin,
		Imp_ArrivalManifestShipmentInfo.Destination,
		Imp_ArrivalManifestShipmentInfo.NatureOfGoodsDescription,
		Imp_ArrivalManifestShipmentInfo.Piece,
		Imp_ArrivalManifestShipmentInfo.Weight,
		TTWLBookingInfo.FWB,
		Imp_ArrivalManifestShipmentInfo.ImpArrivalManifestShipmentInfoId,
		Imp_ArrivalManifestULD.ULDNumber,
		Imp_ContainerPalletMessageULD.ContourCode,
		Imp_ContainerPalletMessageULD.Weight,
		TTWLBookingInfo.Source,
		TTWLBookingInfo.DepartureFlightId,
		TTWLBookingInfo.ShipmentNumber,
		TTWLBookingInfo.ShipmentDate,
		TTWLBookingInfo.TOPPrioritySHC	
	having
		(select
				ISNULL(sum(.Exp_LoadedShipmentInfo.Pieces),0)
			from
				Exp_AssignedULDTrolleyToFlight inner join
				Exp_LoadedShipmentInfo
					on Exp_AssignedULDTrolleyToFlight.AssUldTrolleyId = Exp_LoadedShipmentInfo.AssUldTrolleyId inner join
				Shipment_Master
					on Exp_LoadedShipmentInfo.ShipmentId = Shipment_Master.ShipmentId
			where
				Exp_AssignedULDTrolleyToFlight.FlightId = TTWLBookingInfo.DepartureFlightId and
				Shipment_Master.ShipmentNumber = TTWLBookingInfo.ShipmentNumber and
				Shipment_Master.ShipmentDate = TTWLBookingInfo.ShipmentDate
		) < (select
					ISNULL(sum(innerShipmentInfo.Piece),0)
				from
					Imp_ArrivalManifestShipmentInfo innerShipmentInfo
				where
					innerShipmentInfo.BookingFlightId = TTWLBookingInfo.DepartureFlightId and
					innerShipmentInfo.ShipmentNumber = TTWLBookingInfo.ShipmentNumber and
					innerShipmentInfo.ShipmentDate = TTWLBookingInfo.ShipmentDate	
			)
	union
	select
		TTWLBookingInfo.DepartureDateTime,
		TTWLBookingInfo.DepartureFlightKey,
		TTWLBookingInfo.DepartureFlightDate,
		TTWLBookingInfo.DepartureFlightTime,
		TTWLBookingInfo.DepartureFlightOffPoint,
		TTWLBookingInfo.DepartureFlightSegment,
		TTWLBookingInfo.DepartureFlightSegmentOrder,
		TTWLBookingInfo.DepartureFlightSTD,
		TTWLBookingInfo.DepartureFlightStatus,
		TTWLBookingInfo.ArrivalFlightKey,
		TTWLBookingInfo.ArrivaleFlightDate,
		TTWLBookingInfo.ArrivalFlightTime,
		TTWLBookingInfo.ArrivalFlightOffPoint,
		TTWLBookingInfo.ArrivalFlightSegment,
		TTWLBookingInfo.ArrivaleFlightSegmentOrder,
		TTWLBookingInfo.ArrivalFlightSTA,
		TTWLBookingInfo.DisplayShipmentNumber,
		Shipment_Master.Origin,
		Shipment_Master.Destination,
		Shipment_Master.NatureOfGoodsDescription,
		Shipment_Master.Pieces  as TTWLPieces,
		Shipment_Master.Weight  as TTWLWeigtht,
		TTWLBookingInfo.FWB,
		isnull((select
						TOP 1 Exp_LoadedShipmentSHCInfo.SpecialHandlingCode
					from
						Exp_LoadedShipmentSHCInfo inner join
						Mst_SpecialHandlingCode
							on Exp_LoadedShipmentSHCInfo.SpecialHandlingCode = Mst_SpecialHandlingCode.SpecialHandlingCode
					where
						Exp_LoadedShipmentSHCInfo.LoadedShipmentInfoId = Exp_LoadedShipmentInfo.LoadedShipmentInfoId
					order by
						Mst_SpecialHandlingCode.SpecialHandlingPriority
				), TTWLBookingInfo.TOPPrioritySHC) as TOPPrioritySHC,
		Exp_LoadedShipmentInfo.Pieces TTWLLoadedPieces,
		Exp_LoadedShipmentInfo.Weight TTWLLoadedWeight,
		isnull(Exp_AssignedULDTrolleyToFlight.ULDTrolleyNumber, 'BULK') ULDNumber,
		'GK' as GrossWeight,	
		Exp_DLSULDTrolley.HeightCode as ContourCode,
		Exp_DLSULDTrolley.ActualWeight as ULDGrossWeight,
		case
			when (select 
						sum(innerLoadedShipmentInfo.Pieces)
					from
						Exp_LoadedShipmentInfo innerLoadedShipmentInfo inner join
						Exp_AssignedULDTrolleyToFlight innerAssignedULDTrolleyToFlight
							on innerLoadedShipmentInfo.AssUldTrolleyId = innerAssignedULDTrolleyToFlight.AssUldTrolleyId
					where
						innerAssignedULDTrolleyToFlight.FlightId = Exp_AssignedULDTrolleyToFlight.FlightId and
						innerLoadedShipmentInfo.ShipmentId = Shipment_Master.ShipmentId) = TTWLBookingInfo.BookedPieces then
				'Y' 
			else
				'N'
			end as Ready,
		case
			when (select 
						sum(innerLoadedShipmentInfo.Pieces)
					from
						Exp_LoadedShipmentInfo innerLoadedShipmentInfo inner join
						Exp_AssignedULDTrolleyToFlight innerAssignedULDTrolleyToFlight
							on innerLoadedShipmentInfo.AssUldTrolleyId = innerAssignedULDTrolleyToFlight.AssUldTrolleyId
					where
						innerAssignedULDTrolleyToFlight.FlightId = Exp_AssignedULDTrolleyToFlight.FlightId and
						innerLoadedShipmentInfo.ShipmentId = Shipment_Master.ShipmentId) <> TTWLBookingInfo.BookedPieces then
				'ER' 
			else
				null
			end as ERR,
			case when TTWLBookingInfo.Source = 'FFR' then 'Y' else null end as FTT,
		null Finalized,
		case when (Exp_AssignedULDTrolleyToFlight.AssUldTrolleyId is null)
	    then 0 else 1 end Assigned,
	    Case when Exp_LoadedShipmentInfo.TTWLFinalized = 1 then 'F' else '' end as ShipmentFinalized,
		Case when Exp_AssignedULDTrolleyToFlight.TTWLFinalized = 1 then 'F' else '' end as UldFinalized
	from
		Exp_AssignedULDTrolleyToFlight inner join
		Exp_LoadedShipmentInfo
			on Exp_AssignedULDTrolleyToFlight.AssUldTrolleyId = Exp_LoadedShipmentInfo.AssUldTrolleyId inner join
		Shipment_Master
			on Exp_LoadedShipmentInfo.ShipmentId = Shipment_Master.ShipmentId inner join
		TTWLBookingInfo
			on Shipment_Master.ShipmentNumber = TTWLBookingInfo.ShipmentNumber and
				Shipment_Master.ShipmentDate = TTWLBookingInfo.ShipmentDate and
				Exp_AssignedULDTrolleyToFlight.FlightId = TTWLBookingInfo.DepartureFlightId left join
		Exp_DLS
			on Exp_DLS.FlightId = Exp_AssignedULDTrolleyToFlight.FlightId left join
		Exp_DLSULDTrolley
			on Exp_DLS.DLSId = Exp_DLSULDTrolley.DLSId and
				Exp_AssignedULDTrolleyToFlight.ULDTrolleyNumber = Exp_DLSULDTrolley.ULDTrolleyNumber
)TTWLBookingInfo
order by
	TTWLBookingInfo.DepartureDateTime,
    TTWLBookingInfo.DepartureFlightSegmentOrder]]></xml-property>
        </oda-data-set>
    </data-sets>

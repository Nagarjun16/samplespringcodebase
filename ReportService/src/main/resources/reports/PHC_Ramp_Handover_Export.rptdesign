            <xml-property name="queryText"><![CDATA[----------------------
--Export Ramp Handover 
-----------------------


select
       *
from(
       select 
              Flt_OperativeFlight.CarrierCode,
              Flt_OperativeFlight.FlightNumber, 
              Flt_OperativeFlight.FlightKey,
              format(coalesce(Flt_OperativeFlight_legs.DateATD, Flt_OperativeFlight_Legs.DateSTD),'dd-MM-yyyy HH:mm') as DateSTD,
              case 
                      when Exp_HandOverContainerTrolleyInfo.Ocs = 1 then 'O'
                      when Exp_HandOverContainerTrolleyInfo.HandCarry = 1 then 'H'
                      else 'U' end as HandOverType,
              case 
                      when Exp_HandOverContainerTrolleyInfo.Ocs = 1 then 'OCS'
                      when Exp_HandOverContainerTrolleyInfo.HandCarry = 1 then 'HandCarry'
                      else Exp_HandOverContainerTrolleyInfo.ContainerTrolleyNumber end as HandOverReferenceNumber,    
              substring(Exp_HandOverContainerTrolleyInfo.DriverId,1,16) as DriverID,
              Flt_OperativeFlight.ParkingBayDepAircraft as BayDropOff,
              Exp_HandOver.HandedOverAt as HandlingAreaCode,
              --expHandOvrContnrTrolyInfo.HandCarry,
              DATEDIFF(minute, CONVERT(VARCHAR(16),DATEADD(hour,8,Exp_HandOver.CompletedAt),120), coalesce(Flt_OperativeFlight_Legs.DateATD, Flt_OperativeFlight_Legs.DateSTD)) as datediff,
              UPPER(FORMAT(( select convert (datetime,SWITCHOFFSET(converT(datetimeoffset,getDate()),
                               (SELECT current_utc_offset FROM   sys.time_zone_info WHERE  NAME = 
                                            (select ParameterValueChar from App_SystemParameters where upper(ParameterCode)=upper('Station_timezone'))))) 
                                            ), 'dd MMM yyyy')) as currentDate,
              ISNULL(Exp_HandOver.HandedOverAt,'') as CargoTerminalCode,
              Exp_HandOver.ReleaseLocation,
              upper(Exp_HandOver.HandedOverBy) HandedOverBy,
              FORMAT(DATEADD(hour,8,Exp_HandOver.CompletedAt), 'dd-MM-yyyy HH:mm') as HandOverDate,
       CONVERT(VARCHAR(16),ISNULL(DATEADD(hour,8,Exp_HandOver.LastUpdatedDateTime),DATEADD(hour,8,Exp_HandOver.CreatedDateTime)),120) as LastUpdatedOn
       from 
              Exp_HandOver inner join 
              Exp_HandOverContainerTrolleyInfo
                      on Exp_HandOver.ExpHandOverId = Exp_HandOverContainerTrolleyInfo.ExpHandOverId inner join 
              Flt_OperativeFlight
                      on Exp_HandOver.FlightId = Flt_OperativeFlight.Flight_ID inner join
              Flt_OperativeFlight_Legs
                      on Flt_OperativeFlight.Flight_ID = Flt_OperativeFlight_Legs.Flight_ID and
                             Flt_OperativeFlight_Legs.FlightBoardPoint = 'SIN'
       where 
              Exp_HandOver.CompletedAt is not null and 
              cast(Flt_OperativeFlight_Legs.DateSTD As Datetime) between ? and ? and
              Exp_HandOver.HandedOverAt = (select ParameterValueChar from App_SystemParameters where ParameterCode = 'PHC_TERMINAL')
       union
       select       
              Flt_OperativeFlight.CarrierCode,
              Flt_OperativeFlight.FlightNumber, 
              Flt_OperativeFlight.FlightKey,
              format(coalesce(Flt_OperativeFlight_Legs.DateATD, Flt_OperativeFlight_Legs.DateSTD),'dd-MM-yyyy HH:mm') as  DateSTD,
              case 
                      when Exp_CheckInContainerTrolleyInfo.Ocs = 1 then 'O'
                      when Exp_CheckInContainerTrolleyInfo.HandCarry = 1 then 'H'
                      else 'U' end as HandOverType,
              case 
                      when Exp_CheckInContainerTrolleyInfo.Ocs = 1 then 'OCS'
                      when Exp_CheckInContainerTrolleyInfo.HandCarry = 1 then 'HandCarry'
                      else Exp_CheckInContainerTrolleyInfo.ContainerTrolleyNumber end as HandOverReferenceNumber,    
              substring(Exp_CheckInContainerTrolleyInfo.DriverId, 1, 16),
              Flt_OperativeFlight.ParkingBayDepAircraft as BayDropOff,
              Exp_CheckInContainerTrolleyInfo.HandedOverAt as HandlingAreaCode,
              DATEDIFF(minute, CONVERT(VARCHAR(16),DATEADD(hour,8,Exp_CheckInContainerTrolleyInfo.HandedOverTime),120), coalesce(Flt_OperativeFlight_Legs.DateATD, Flt_OperativeFlight_Legs.DateSTD)) as datediff,
              UPPER(FORMAT(( select convert (datetime,SWITCHOFFSET(converT(datetimeoffset,getDate()),
                             (SELECT current_utc_offset FROM   sys.time_zone_info  WHERE  NAME = 
                                    (select ParameterValueChar from App_SystemParameters where upper(ParameterCode)=upper('Station_timezone'))))) 
                                           ), 'dd MMM yyyy')) as currentDate,
              ISNULL(Exp_CheckInContainerTrolleyInfo.HandedOverAt, '') as CargoTerminalCode,
              Exp_CheckInContainerTrolleyInfo.HandedOverAt as ReleaseLocation,
              Exp_CheckInContainerTrolleyInfo.HandedOverBy,
       FORMAT(DATEADD(hour,8,Exp_CheckInContainerTrolleyInfo.HandedOverTime), 'dd-MM-yyyy HH:mm') as HandOverDate,
       CONVERT(VARCHAR(16),ISNULL(DATEADD(hour,8,Exp_CheckInContainerTrolleyInfo.LastUpdatedDateTime), 
       DATEADD(hour,8,Exp_CheckInContainerTrolleyInfo.CreatedDateTime)),120) as LastUpdateOn
       from 
              Exp_CheckInOffloadReturntoWarehouse inner join 
              Exp_CheckInContainerTrolleyInfo
                      on Exp_CheckInOffloadReturntoWarehouse.ExpCheckInId = Exp_CheckInContainerTrolleyInfo.ExpCheckInId inner join 
              Flt_OperativeFlight 
                      on Exp_CheckInOffloadReturntoWarehouse.FlightId = Flt_OperativeFlight.Flight_ID inner join 
              Flt_OperativeFlight_Legs
                      on Flt_OperativeFlight.Flight_ID = Flt_OperativeFlight_Legs.Flight_ID and
                             Flt_OperativeFlight_Legs.FlightBoardPoint = 'SIN'
       where 
              Exp_CheckInContainerTrolleyInfo.HandedOverTime is not null and      
              cast(coalesce(Flt_OperativeFlight_legs.DateATD, Flt_OperativeFlight_Legs.DateSTD) As Datetime) between ? and ? and
              Exp_CheckInContainerTrolleyInfo.HandedOverAt = (select ParameterValueChar from App_SystemParameters where ParameterCode = 'PHC_TERMINAL')
)HandOverInfo
order by
       HandOverInfo.DateSTD,
       HandOverInfo.FlightKey
]]></xml-property>
        </oda-data-set>

            <xml-property name="queryText"><![CDATA[select format(cast(? as datetime),'HH:mm') as FromDate, format(cast(? as datetime),'HH:mm') as ToDate,
UPPER(FORMAT(( select convert
		(datetime,SWITCHOFFSET(converT(datetimeoffset,getDate()),
		(SELECT current_utc_offset 
				FROM   sys.time_zone_info 
